{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "overrides": [
    {
      "files": ["**/*.css", "**/*.scss", "**/*.html"],
      "options": {
        "singleQuote": false
      }
    }
  ],
  "jsxSingleQuote": false,
  "printWidth": 100,
  "trailingComma": "none",
  "vueIndentScriptAndStyle": true
}
